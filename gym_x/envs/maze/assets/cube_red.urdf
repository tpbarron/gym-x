<?xml version="1.0"?>
<robot name="cube">

  <link name="cube">

    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="1 1 1"/>
      </geometry>
      <material name="solid_color">
        <color rgba="1 0 0 1"/>
      </material>
    </visual>

